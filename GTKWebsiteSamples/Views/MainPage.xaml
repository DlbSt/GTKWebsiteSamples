<?xml version="1.0" encoding="utf-8" ?>
<ContentPage x:Class="GTKWebsiteSamples.Views.MainPage"
             xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:prism="http://prismlibrary.com"
             Title="{Binding Title, Mode=OneTime}"
             prism:ViewModelLocator.AutowireViewModel="True">
    <ScrollView>
        <StackLayout>
            <Button Command="{Binding OnNavigateToDialogPage, Mode=OneTime}"
                    Text="Dialogs - Message Boxes and Simple Dialogs" />
            <Button Command="{Binding OnNavigateToLicensePage, Mode=OneTime}"
                    Text="Licenses" />
        </StackLayout>
    </ScrollView>
</ContentPage>